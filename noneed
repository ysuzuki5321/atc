using namespace std;
typedef long long ll;
typedef pair<int, int> pii;

const ll INF = 1000000007;

const int MAX = 210000;
const int MOD = 1000000007;

long long fac[MAX], finv[MAX], inv[MAX];
void COMinit() {
	fac[0] = fac[1] = 1;
	finv[0] = finv[1] = 1;
	inv[1] = 1;
	for (int i = 2; i < MAX; i++) {
		fac[i] = fac[i - 1] * i % MOD;
		inv[i] = MOD - inv[MOD%i] * (MOD / i) % MOD;
		finv[i] = finv[i - 1] * inv[i] % MOD;
	}
}
ll gcd(ll a, ll b) {
	if (b == 0) return a;
	return gcd(b, a % b);
}
int pr[100010];
void uini(int n) {
	for (size_t i = 0; i <= n; i++)
	{
		pr[i] = i;
	}
}

int parent(int x) {
	if (x == pr[x]) return x;
	return pr[x] = parent(pr[x]);
}

bool unit(int x, int y) {
	int px = parent(x);
	int py = parent(y);

	if (px == py) return false;
	if (px < py) {
		pr[py] = px;
	}
	else {
		pr[px] = py;
	}
	return true;
}
ll ar[40];
int memo[1001][1001];
int revm[1001];

bool ok[5010];
bool use[301][301];
void solv() {
	ll n,k;
	cin >> n >> k;
	ll a[5010];
	for (size_t i = 0; i < n; i++)
	{
		cin >> a[i];
	}
	sort(a, a + n);
	ll sum = 0;
	ll ni = 0;

	for (size_t i = 0; i < n; i++)
	{
		sum += a[i];
		if (sum >= k) {
			for (; ni < i; ni++)
			{
				sum -= a[ni];
				if (sum < k) {
					ok[ni] = true;
				}
			}
		}
	}
	ll res = 0;
	for (size_t i = 0; i < n; i++)
	{
		res += !ok[i];
		if (ok[i]) break;
	}

	cout << res << endl;
}




int main() {
	COMinit();
	solv();

	return 0;
}
